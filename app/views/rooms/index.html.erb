<h1>Rooms#index</h1>
<p>Find me in app/views/rooms/index.html.erb</p>
<% @rooms.each do |room|%>
  <p><%= room.info %></p>
<% end %>

<%= link_to 'New Room', new_room_path%>
<br />
<br />
<%= form_for @room, url:"/rooms/find" do|f|%>
  <%= f.label :code%>
  <%= f.text_field :code%>
  <%=f.submit 'Join Room'%>
<% end %>